{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@now/static-build" },
    { "src": "api/*.js", "use": "@now/node" }
  ],
  "routes": [
    {
      "src": "^/api/airtable/(.*)",
      "dest": "/api/airtable.js"
    }
  ],
  "env": {
    "API_KEY": "@pb_apikey",
    "BASE":"@pb_base"
  }
}